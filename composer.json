{
    "name": "codesinging/think-sae",
    "description": "A ThinkPHP package for SAE support",
    "license": "MIT",
    "authors": [
        {
            "name": "codesinging",
            "email": "codesinging@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "CodeSinging\\ThinkSae\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "CodeSinging\\ThinkSae\\ThinkSaeService"
            ],
            "config": {
                "sae": "src/config.php"
            }
        }
    }
}
